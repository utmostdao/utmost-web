{
  "name": "utmost-swap",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "yarn copy_preview_env && nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "dev:https": "yarn nuxt -p 4001 | local-ssl-proxy --key certs/localhost.key --cert certs/localhost.crt --source 4000 --target 4001",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix && yarn lint:style --fix",
    "prepare": "husky install",
    "test": "jest",
    "copy_prod_env": "yarn rm_env && shx cp .env.production .env",
    "copy_preview_env": "yarn rm_env && shx cp .env.preview .env",
    "rm_env": "shx rm -f .env",
    "sync_api": "yarn ts-node scripts/sync.ts",
    "generate_api": "yarn openapi-typescript scripts/swagger.json --output types/swagger.ts && yarn ts-node scripts/generate.ts",
    "cross_generate_api": " yarn openapi-typescript scripts/crossSwagger.json --output types/crossSwagger.ts"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.**": "prettier --check --ignore-unknown"
  },
  "dependencies": {
    "@nuxtjs/axios": "5.13.6",
    "@nuxtjs/i18n": "7.2.2",
    "@nuxtjs/pwa": "3.3.5",
    "@web3-onboard/injected-wallets": "2.10.9",
    "@web3-onboard/walletconnect": "2.4.4",
    "color": "4.2.3",
    "core-js": "3.23.3",
    "dayjs": "^1.11.6",
    "decimal.js": "^10.3.1",
    "element-ui": "2.15.9",
    "ethers": "5.7.0",
    "floating-vue": "1.0.0-beta.18",
    "mersenne-twister": "1.1.0",
    "nuxt": "2.15.8",
    "nuxt-breakpoints": "^1.1.9",
    "nuxt-breakpoints-ssr": "0.1.4",
    "nuxt-typed-vuex": "0.3.1",
    "overlayscrollbars": "1.13.2",
    "overlayscrollbars-vue": "0.2.2",
    "qrcode": "^1.5.3",
    "vue": "2.7.2",
    "vue-debounce": "^4.0.0",
    "vue-final-modal-types": "2.4.4",
    "vue-server-renderer": "2.7.2",
    "vue-template-compiler": "2.7.2",
    "web3-onboard-core": "2.3.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.18.2",
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@nuxt/devtools": "^1.0.4",
    "@nuxt/image": "^0.7.1",
    "@nuxt/types": "2.15.8",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/color-mode": "2",
    "@nuxtjs/eslint-config-typescript": "8.0.0",
    "@nuxtjs/eslint-module": "3.1.0",
    "@nuxtjs/style-resources": "1.2.1",
    "@nuxtjs/stylelint-module": "4.1.0",
    "@types/color": "3.0.3",
    "@types/jest": "28.1.4",
    "@types/mersenne-twister": "1.1.2",
    "@types/node-fetch": "2.6.2",
    "@types/overlayscrollbars": "1.12.1",
    "@vue/test-utils": "1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "27.5.1",
    "babel-plugin-component": "1.1.1",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-nuxt": "3.2.0",
    "eslint-plugin-vue": "8.7.1",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "^12.5.0",
    "local-ssl-proxy": "^2.0.5",
    "openapi-typescript": "5.4.0",
    "postcss-html": "1.4.1",
    "prettier": "2.7.1",
    "replace-in-file": "6.3.5",
    "sass": "1.53.0",
    "sass-loader": "10.3.0",
    "shx": "0.3.4",
    "stylelint": "14.9.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard": "24.0.0",
    "ts-jest": "27.1.5",
    "ts-morph": "15.1.0",
    "vue-jest": "3.0.7",
    "webpack": "4.46.0"
  },
  "browserslist": {
    "production": [
      "chrome >= 67",
      "edge >= 79",
      "firefox >= 68",
      "opera >= 54",
      "safari >= 14"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
